{
    "name": "nilead/nilead",
    "description": "The \"NiLead Standard Edition\" distribution",
    "authors": [
        {
            "name": "Raine Ng",
            "email": "vunguyen@rubikin.com",
            "homepage": "http://rubikin.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.2.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/data-fixtures": "1.0.*@dev",
        "stof/doctrine-extensions-bundle": "@dev",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "symfony/monolog-bundle": "2.2.*",
        "symfony/web-profiler-bundle": "2.2.*@dev",
        "sensio/distribution-bundle": "2.2.*",
        "sensio/framework-extra-bundle": "2.2.*",
        "sensio/generator-bundle": "2.2.*",
        "jms/i18n-routing-bundle": "dev-master",
        "jms/translation-bundle": "1.1.*@dev",
        "jms/security-extra-bundle": "1.4.*",
        "jms/di-extra-bundle": "1.3.*",
        "gedmo/doctrine-extensions": "2.3.*@dev",
        "friendsofsymfony/user-bundle": "*",
        "nelmio/alice": "1.4.*@dev",
        "sylius/cart-bundle": "@dev",
        "sylius/money-bundle": "@dev",
        "sylius/resource-bundle": "@dev",
        "sylius/taxonomies-bundle": "@dev",
        "sylius/inventory-bundle": "@dev",
        "sylius/sales-bundle": "@dev",
        "sylius/taxation-bundle": "@dev",
        "sylius/inventory-bundle": "@dev",
        "mathiasverraes/money": "@dev",
        "white-october/pagerfanta-bundle": "@dev",
        "pagerfanta/pagerfanta": "@dev",
        "knplabs/knp-menu": "2.0.*@dev",
        "knplabs/knp-menu-bundle": "2.0.*@dev",
        "nilead/core-bundle": "0.1.*@dev",
        "nilead/pricing-bundle": "@dev",
        "nilead/contact-bundle": "@dev",
        "nilead/menu-bundle": "@dev",
        "nilead/payments-bundle": "@dev",
        "nilead/shipments-bundle": "@dev",
        "nilead/inventory-bundle": "@dev",
        "nilead/product-bundle": "@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "alpha",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}